numero_da_tabuada = int(input('informe o número: '))

valor = 1
i = 1

while (i > 0 and i <= 10):
    if(numero_da_tabuada > 10):
        print('o número deve ser menor de dez')
        numero_da_tabuada = input('informe o número: ')

    valor = numero_da_tabuada * i
    if i <= 10:
        print(f'{numero_da_tabuada} X {i} = {valor}')
    i += 1
